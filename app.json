{
  "expo": {
    "name": "Malibandhan",
    "slug": "Malibandhan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "scheme": "Malibandhanmobileapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/flash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF0F5"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/flash.png",
        "backgroundColor": "#FFF0F5"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.maheshkadam.Malibandhan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1cc65a09-b6f4-4c70-bf81-0147c8934bd8"
      }
    },
    "owner": "harshalladukar"
  }
}